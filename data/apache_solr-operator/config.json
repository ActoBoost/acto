{
    "deploy": {
        "steps": [
            {
                "apply": {
                    "file": "data/apache_solr-operator/crd.yaml"
                }
            },
            {
                "apply": {
                    "file": "data/apache_solr-operator/bundle.yaml"
                }
            },
            {
                "apply": {
                    "file": "data/apache_solr-operator/operator.yaml",
                    "operator": true
                }
            }
        ]
    },
    "crd_name": "solrclouds.solr.apache.org",
    "custom_module": "data.apache_solr-operator.custom_mapping",
    "kubernetes_version": "v1.28.0",
    "seed_custom_resource": "data/apache_solr-operator/cr.yaml"
}
